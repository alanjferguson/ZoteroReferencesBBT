}

@article{xuLongspanBridgesEnhanced2017,
  title = {Long-Span Bridges: {{Enhanced}} Data Fusion of {{GPS}} Displacement and Deck Accelerations},
  shorttitle = {Long-Span Bridges},
  author = {Xu, Yan and Brownjohn, James Mark William and Hester, David and Koo, Ki Young},
  date = {2017-09},
  journaltitle = {Engineering Structures},
