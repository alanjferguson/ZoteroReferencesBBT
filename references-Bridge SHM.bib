@article{makkialamdariFrequencyDomainDecompositionbased2019,
  title = {Frequency Domain Decomposition-Based Multisensor Data Fusion for Assessment of Progressive Damage in Structures},
  author = {Makki~Alamdari, Mehrisadat and Anaissi, Ali and Khoa, Nguyen L. D. and Mustapha, Samir},
  date = {2019},
  journaltitle = {Structural Control and Health Monitoring},
