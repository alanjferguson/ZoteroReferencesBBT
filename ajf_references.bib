@article{huseynovBridgeDamageDetection2020,
  ids = {huseynovBridgeDamageDetection,huseynovBridgeDamageDetection2020a},
  title = {Bridge Damage Detection Using Rotation Measurements – {{Experimental}} Validation},
  author = {Huseynov, F. and Kim, C. and OBrien, E. J. and Brownjohn, J. M. W. and Hester, D. and Chang, K. C},
  date = {2020-01-01},
  journaltitle = {Mechanical Systems and Signal Processing},
  shortjournal = {Mechanical Systems and Signal Processing},
  volume = {135},
  pages = {106380},
  issn = {0888-3270},
  doi = {10.1016/j.ymssp.2019.106380},
  url = {http://www.sciencedirect.com/science/article/pii/S0888327019306016},
  urldate = {2020-02-04},
  abstract = {This paper presents a novel bridge condition assessment methodology using direct rotation measurements. Initially, numerical analyses are carried out to develop the theoretical basis of the proposed bridge damage detection methodology. As a result of this study the difference in rotation influence lines obtained for healthy and damaged bridge states is proposed as a damage indicator. The sensitivity of rotation to damage and the effect of sensor locations on sensor sensitivities are investigated. Subsequently, extensive laboratory experiments are conducted on a 5.4 m long simply supported bridge structure in an effort to validate the results from the numerical analyses. The test structure is instrumented with rotation sensors and axle detectors and loaded with a four-axle moving vehicle. In this study, rotations are measured using high grade uniaxial accelerometers. The procedure of measuring rotations using accelerometers is explained within the scope of this study. To test the robustness of the proposed bridge condition assessment methodology, a wide range of single and multiple damage scenarios is investigated and the results from this study show that the proposed methodology can successfully identify damage on a bridge. For the model considered, damage as low as 7\% change in stiffness over an extent of 2.5\% bridge span is shown to be detectable.},
  file = {/Volumes/GoogleDrive/My Drive/Zotero Papers/huseynov_et_al_2020_bridge_damage_detection_using_rotation_measurements_–_experimental_validation.pdf;/Volumes/GoogleDrive/My Drive/Zotero Papers/huseynov_et_al_2020_bridge_damage_detection_using_rotation_measurements_–_experimental_validation2.pdf;/Volumes/GoogleDrive/My Drive/Zotero Papers/huseynov_et_al_2020_bridge_damage_detection_using_rotation_measurements_–_experimental_validation3.pdf;/Users/alanferguson/Zotero/storage/2XKQLL6X/home.html;/Users/alanferguson/Zotero/storage/KMRXJ9CX/S0888327019306016.html},
  keywords = {Bridge damage detection,Health monitoring,Inclinometers,Influence line,Rotation measurements,SHM},
