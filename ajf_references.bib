@inproceedings{glisicStructuralHealthMonitoring2008,
  title = {Structural Health Monitoring Method for Curved Concrete Bridge Box Girders},
  booktitle = {Sensors and {{Smart Structures Technologies}} for {{Civil}}, {{Mechanical}}, and {{Aerospace Systems}} 2008},
  author = {Glišić, Branko and Posenato, Daniele and Inaudi, Daniele and Figini, Angelo},
