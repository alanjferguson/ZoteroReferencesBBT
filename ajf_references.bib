@article{orcesiOptimizationBridgeManagement2010,
  title = {Optimization of {{Bridge Management}} under {{Budget Constraints}}: {{Role}} of {{Structural Health Monitoring}}},
  shorttitle = {Optimization of {{Bridge Management}} under {{Budget Constraints}}},
  author = {Orcesi, André D. and Frangopol, Dan M.},
  date = {2010-01-01},
