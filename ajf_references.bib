  file = {/Volumes/GoogleDrive/My Drive/Zotero Papers/li_et_al_2015_damage_detection_with_streamlined_structural_health_monitoring_data.pdf;/Volumes/GoogleDrive/My Drive/Zotero Papers/li_et_al_2015_damage_detection_with_streamlined_structural_health_monitoring_data2.pdf;/Users/alanferguson/Zotero/storage/M8ZR5PZW/display.html},
