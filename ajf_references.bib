  keywords = {atomic clocks,Atomic clocks,Chip-scale atomic clock,chip-scale atomic clock utilization,CSAC,electric power grid,Frequency measurement,Global Positioning System,global positioning system signal loss,Global Positioning System-based phasor measurement units,GPS receivers,GPS timing,GPS-based PMU,grid status monitoring,phasor measurement,phasor measurement unit,Phasor measurement units,power grids,power system,radio receivers,signal lock,Synchronization,synchrophasor measurements,time synchronization,Yttrium},
  number = {5}
}

