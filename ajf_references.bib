  file = {/Volumes/GoogleDrive/My Drive/Zotero Papers/oskoui_et_al_2020_method_and_sensor_for_monitoring_weight_of_trucks_in_motion_based_on_bridge.pdf;/Users/alanferguson/Zotero/storage/949YNIHB/15732479.2019.html},
  keywords = {axle weight,Bridge weigh in motion,fibre Bragg grating sensors,gross vehicle weight,influence lines,rotation sensor},
