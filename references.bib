@article{ye_technology_2020,
  title = {Technology for Condition and Performance Evaluation of Highway Bridges},
  author = {Ye, Shi and Lai, Xiangang and Bartoli, Ivan and Aktan, A. Emin},
  date = {2020-05-10},
  journaltitle = {Journal of Civil Structural Health Monitoring},
  shortjournal = {J Civil Struct Health Monit},
