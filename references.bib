@article{hackell_threetier_2016,
  title = {Three-{{Tier Modular Structural Health Monitoring Framework Using Environmental}} and {{Operational Condition Clustering}} for {{Data Normalization}}: {{Validation}} on an {{Operational Wind Turbine System}}},
  shorttitle = {Three-{{Tier Modular Structural Health Monitoring Framework Using Environmental}} and {{Operational Condition Clustering}} for {{Data Normalization}}},
  author = {Hackell, Moritz W. and Rolfes, Raimund and Kane, Michael B. and Lynch, Jerome P.},
