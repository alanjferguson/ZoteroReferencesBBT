@article{karatas_fibre_,
  title = {Fibre {{Bragg}} Grating Sensor Applications for Structural Health Monitoring},
  author = {Karatas, Cansu and Degerliyurt, Boray and Yaman, Yavuz and Sahin, Melin},
  journaltitle = {Aircraft Engineering and Aerospace Technology},
