@article{sun_structural_2002,
  title = {Structural Damage Assessment Based on Wavelet Packet Transform},
  author = {Sun, Z. and Chang, C.C.},
  date = {2002},
  journaltitle = {Journal of Structural Engineering},
  volume = {128},
