  langid = {english},
  keywords = {beam damage detection,influence lines,structural health monitoring,system identification},
  file = {/Users/ajf/Zotero/storage/H5JF9Z67/Zeinali and Story - 2017 - Framework for Flexural Rigidity Estimation in Eule.pdf;/Users/ajf/Zotero/storage/T8DIX5QC/23.html}
